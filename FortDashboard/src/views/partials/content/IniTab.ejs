<div>
  <h2>INI Management</h2>

  <ul class="nav nav-tabs" id="myTab" role="tablist">
    <% IniData.forEach(function(item, index) { %>
    <li class="nav-item" role="presentation">
      <button
        class="nav-link <%= index === 0 ? 'ButtonColorType' : '' %>"
        id="<%= item.FileName %>-tab"
        data-bs-toggle="tab"
        data-bs-target="#<%= item.FileName %>"
        role="tab"
        aria-controls="<%= item.FileName %>"
        aria-selected="<%= index === 0 ? 'true' : 'false' %>"
      >
        <%= item.FileName %>
      </button>
    </li>
    <% }); %>
  </ul>

  <div class="tab-content">
    <% IniData.forEach(function(item, index) { %>
    <div
      class="tab-pane fade <%= index === 0 ? 'show active' : '' %>"
      id="<%= item.FileName %>"
      role="tabpanel"
      aria-labelledby="<%= item.FileName %>-tab"
    >
      <h2><%= item.FileName %></h2>

      <table class="table table-bordered table-hover rounded-table">
        <thead>
          <tr>
            <th>Title</th>
            <th>Body</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% item.Data.forEach(function(singleItem, index2) { %>
          <tr>
            <td><%= singleItem.Title %></td>
            <td>N/A</td>
            <td>
              <button
                class="btn ButtonColorType"
                data-bs-toggle="modal"
                data-bs-target="#editModal"
                data-context="ini"
                data-contextId="<%= index %>"
                data-item="<%= index2 %>"
              >
                Edit
              </button>
            </td>
          </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
    <% }); %>
  </div>
</div>

<script src="/public/js/Content/IniNewTab.js"></script>

{
  "name": "fortlauncherv2",
  "private": false,
  "version": "0.0.0",
  "type": "commonjs",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "vite",
    "obfuscate": "node obfuscate.js",
    "build": "vue-tsc && vite build && npm run obfuscate && electron-builder --win",
    "preview": "vite preview"
  },
  "build": {
    "directories": {
      "output": "dist"
    },
    "appId": "FortLauncher",
    "productName": "FortLauncher",
    "win": {},
    "asar": true,
    "files": [
      "out/**/*",
      "resources/**/*"
    ],
    "protocols": [
      {
        "name": "FortLauncher",
        "schemes": [
          "FortLauncher"
        ]
      }
    ]
  },
  "dependencies": {
    "axios": "^1.8.3",
    "dotenv": "^16.4.7",
    "ini": "^5.0.0",
    "javascript-obfuscator": "^4.1.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/ini": "^4.1.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.0.26"
  }
}

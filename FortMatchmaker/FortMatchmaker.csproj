<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Configurations>Debug;Release;HTTPS;DEVELOPMENT</Configurations>
    <Platforms>AnyCPU;x64;ARM32</Platforms>
    <BaseOutputPath>..\..\FortBackend\FortBackend\bin\x64</BaseOutputPath>
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>

  <ItemGroup>
    <_ContentIncludedByDefault Remove="Resources\Config.json" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\FortLibrary\FortLibrary.csproj" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

</Project>
